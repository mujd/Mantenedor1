<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cliente" table="cliente">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="rut" type="string" column="rut" length="12" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombres" type="string" column="nombres" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidop" type="string" column="apellidoP" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidom" type="string" column="apellidoM" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="direccion" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fono" type="string" column="fono" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
